{
    "name": "melogail/laravel-tags",
    "description": "Tagging system for laravel 5.*/6.* framework",
    "keywords": ["tags", "taggable", "laravel"],
    "type": "library",
    "homepage": "https://github.com/melogail/laravel-tags",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Mohamed Elogail",
            "email": "moh.elogail@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Melogail\\LaravelTags\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
